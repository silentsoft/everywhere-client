<StackPane style="-fx-background-image: url('/images/view/login/blur.jpg'); -fx-background-repeat: stretch; -fx-background-size: cover; -fx-background-position: center center;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.silentsoft.everywhere.client.view.login.LoginViewerController">
   <children>
      <VBox alignment="CENTER">
         <children>
            <Pane />
            <HBox alignment="CENTER">
               <children>
                  <Pane />
                  <AnchorPane prefHeight="485.0" prefWidth="910.0">
                     <children>
                        <Label focusTraversable="false" layoutX="327.0" layoutY="94.0" text="Member Login" textFill="#7c7c7c">
