<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<Pane minHeight="350.0" minWidth="750.0" prefHeight="350.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.silentsoft.everywhere.client.view.main.upload.UploadViewerController">
   <children>
      <TableView fx:id="tableView" layoutY="40.0" prefHeight="250.0" prefWidth="750.0">
        <columns>
          <TableColumn fx:id="colFilePath" editable="false" prefWidth="612.0" text="File Path" />
          <TableColumn fx:id="colSize" editable="false" prefWidth="136.0" text="Size" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btnUpload" defaultButton="true" layoutX="602.0" layoutY="303.0" mnemonicParsing="false" onAction="#upload_OnMouseClick" prefHeight="33.0" prefWidth="134.0" text="Upload" />
      <Button fx:id="btnSelect" layoutX="14.0" layoutY="303.0" mnemonicParsing="false" onAction="#select_OnMouseClick" prefHeight="33.0" prefWidth="134.0" text="Select" />
      <Pane minHeight="40.0" minWidth="750.0" prefHeight="40.0" prefWidth="750.0" style="-fx-background-color: #f2f2f2;">
         <children>
            <Label layoutX="14.0" layoutY="9.0" text="File Upload">
               <font>
                  <Font name="Verdana Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Button fx:id="btnClose" cancelButton="true" layoutX="452.0" layoutY="303.0" mnemonicParsing="false" onAction="#close_OnMouseClick" prefHeight="33.0" prefWidth="134.0" text="Close" />
   </children>
</Pane>
